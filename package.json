{
  "name": "msd-front",
  "version": "0.0.1",
  "private": true,
  "description": "msd-front",
  "author": "Quentin LE CAIGNEC",
  "keywords": [
    "react",
    "next"
  ],
  "scripts": {
    "prepare": "husky install",
    "codegen": "graphql-codegen --config src/graphql/codegen.ts",
    "develop": "next dev --port ${PORT-3002}",
    "develop:mock": "NEXT_PUBLIC_MOCK=true next dev --port ${PORT-3002}",
    "build": "next build",
    "build:mock": "NEXT_PUBLIC_MOCK=true next build",
    "start": "next start --port ${PORT-3002}",
    "export": "next export",
    "clean": "rm -rf .cache .tmp .next .swc dist build out coverage",
    "lint": "npm run lint:eslint && npm run lint:style && npm run lint:prettier",
    "lint:eslint": "eslint . --fix --max-warnings=0 --ignore-path .gitignore --ignore-path .eslintignore",
    "lint:style": "stylelint src/**/*.{css,scss,sass} --fix --max-warnings=0 --ignore-path .gitignore",
    "lint:prettier": "prettier . --write --ignore-unknown --ignore-path .gitignore --ignore-path .prettier.ignore",
    "pretest": "tsc --noEmit",
    "test": "npm run test:jest && npm run test:e2e",
    "test:ci": "npm run lint && npm run pretest && npm run test:jest:ci && npm run test:e2e:ci",
    "test:jest": "jest --watch --coverage",
    "test:jest:ci": "jest --ci --coverage --coverageReporters=text --coverageReporters=cobertura --coverageReporters=html",
    "test:e2e": "npx playwright test",
    "test:e2e:ci": "CI=true npx playwright install --with-deps && CI=true npx playwright test"
  },
  "dependencies": {
    "@apollo/client": "3.7.4",
    "@suezenv/react-theme-components": "2.8.3",
    "chroma-js": "2.4.2",
    "classnames": "2.3.2",
    "date-fns": "2.29.3",
    "graphql": "16.6.0",
    "isomorphic-dompurify": "0.26.0",
    "next": "13.1.2",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-big-calendar": "1.5.2",
    "react-dom": "18.2.0",
    "react-player": "^2.12.0",
    "swr": "2.0.0",
    "web-vitals": "3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@faker-js/faker": "7.6.0",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@graphql-codegen/add": "3.2.3",
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/client-preset": "1.2.6",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-graphql-files-modules": "2.2.1",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@graphql-tools/mock": "8.7.15",
    "@graphql-tools/schema": "9.0.13",
    "@playwright/test": "1.27.1",
    "@svgr/webpack": "6.5.1",
    "@testing-library/dom": "8.20.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/chroma-js": "^2.1.4",
    "@types/dompurify": "2.4.0",
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.26",
    "@types/react-big-calendar": "0.38.3",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "autoprefixer": "10.4.13",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.2",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-playwright": "0.11.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-testing-library": "5.9.1",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-junit": "15.0.0",
    "lint-staged": "13.1.0",
    "mock-match-media": "0.4.2",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "7.8.3",
    "prettier": "2.8.3",
    "sass": "1.57.1",
    "start-server-and-test": "1.15.2",
    "stylelint": "14.16.1",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-order": "6.0.1",
    "stylelint-prettier": "2.0.0",
    "typescript": "4.9.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        "Infinity"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint . --fix --max-warnings=0 --ignore-path .gitignore --ignore-path .eslintignore",
    "*.{css,scss,sass}": "stylelint src/**/*.{css,scss,sass} --fix --max-warnings=0 --ignore-path .gitignore",
    "*": "prettier . --write --ignore-unknown --ignore-path .gitignore --ignore-path .prettier.ignore"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
