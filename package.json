{
  "name": "msd-front",
  "version": "0.0.1",
  "private": true,
  "description": "msd-front",
  "author": "Quentin LE CAIGNEC",
  "keywords": [
    "react",
    "next"
  ],
  "scripts": {
    "prepare": "husky install",
    "codegen": "API_URL=$(grep NEXT_PUBLIC_API_URL .env | cut -d '=' -f2) graphql-codegen --config src/graphql/codegen.ts",
    "develop": "next dev --port 3002",
    "develop:mock": "NEXT_PUBLIC_MOCK=true next dev --port 3002",
    "build": "next build",
    "build:mock": "NEXT_PUBLIC_MOCK=true next build",
    "start": "next start --port 3002",
    "export": "next export",
    "clean": "rm -rf .cache .tmp .next .swc dist build out coverage",
    "lint": "npm run lint:eslint && npm run lint:style && npm run lint:prettier",
    "lint:eslint": "eslint . --fix --max-warnings=0 --ignore-path .gitignore --ignore-path .eslintignore",
    "lint:style": "stylelint src/**/*.{css,scss,sass} --fix --max-warnings=0 --ignore-path .gitignore",
    "lint:prettier": "prettier . --write --ignore-unknown --ignore-path .gitignore --ignore-path .prettier.ignore",
    "pretest": "tsc --noEmit",
    "test": "npm run test:jest && npm run test:e2e",
    "test:ci": "npm run lint && npm run pretest && npm run test:jest:ci && npm run test:e2e:ci",
    "test:jest": "jest --watch --coverage",
    "test:jest:ci": "jest --ci --reporters=jest-junit --reporters=default --coverage --coverageReporters=text --coverageReporters=cobertura --coverageReporters=html",
    "test:e2e": "BASE_URL=$(grep NEXT_PUBLIC_BASE_URL .env | cut -d '=' -f2) npx playwright test",
    "test:e2e:ci": "BASE_URL=$(grep NEXT_PUBLIC_BASE_URL .env | cut -d '=' -f2) npx playwright install --with-deps && npx playwright test"
  },
  "dependencies": {
    "@apollo/client": "3.7.1",
    "@suezenv/react-theme-components": "2.6.11",
    "date-fns": "2.29.3",
    "graphql": "16.6.0",
    "isomorphic-dompurify": "0.23.0",
    "next": "13.0.2",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-big-calendar": "1.5.0",
    "react-dom": "18.2.0",
    "swr": "1.3.0",
    "web-vitals": "3.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@faker-js/faker": "7.6.0",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@graphql-codegen/cli": "2.13.11",
    "@graphql-codegen/client-preset": "1.1.3",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.8.1",
    "@graphql-codegen/typescript-graphql-files-modules": "2.2.1",
    "@graphql-tools/mock": "8.7.10",
    "@graphql-tools/schema": "9.0.8",
    "@playwright/test": "1.27.1",
    "@svgr/webpack": "6.5.1",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/dompurify": "2.3.4",
    "@types/jest": "29.2.2",
    "@types/node": "18.11.9",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.25",
    "@types/react-big-calendar": "0.38.2",
    "@types/react-dom": "18.0.8",
    "autoprefixer": "10.4.13",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-playwright": "0.11.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-testing-library": "5.9.1",
    "husky": "8.0.1",
    "jest": "29.2.2",
    "jest-environment-jsdom": "29.2.2",
    "jest-junit": "14.0.1",
    "lint-staged": "13.0.3",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "7.8.2",
    "prettier": "2.7.1",
    "sass": "1.56.0",
    "start-server-and-test": "1.14.0",
    "stylelint": "14.14.1",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-order": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "typescript": "4.8.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint . --fix --max-warnings=0 --ignore-path .gitignore --ignore-path .eslintignore",
    "*.{css,scss,sass}": "stylelint src/**/*.{css,scss,sass} --fix --max-warnings=0 --ignore-path .gitignore",
    "*": "prettier . --write --ignore-unknown --ignore-path .gitignore --ignore-path .prettier.ignore"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
